  <!-- P I S T O L S -->
  
  <!-- 
      Makarov Russian Standard issue pistol - Normal mag
  -->

  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>EFR_Gun_Makarov</defName>
    <label>Makarov Standard Issue Pistol</label>
    <description>A time-proven PM. Due to the ubiquitous spread of both pistol and ammo it is still being used by Russian Spetznaz, thanks to exceptional reliability, light weight and compact size. The only serious drawback is its lack of a knockdown power.</description>
    <graphicData>
      <texPath>Things/Makarov</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>InteractAutopistol</soundInteract>
    <statBases>
      <WorkToMake>10000</WorkToMake>
      <Mass>0.73</Mass>
      <MaxHitPoints>100</MaxHitPoints>
        <AccuracyTouch>0.98</AccuracyTouch>
        <AccuracyShort>0.70</AccuracyShort>
        <AccuracyMedium>0.31</AccuracyMedium>
        <AccuracyLong>0.09</AccuracyLong>
      <RangedWeapon_Cooldown>0.4</RangedWeapon_Cooldown>
      <Weapon_Bulk>0.3</Weapon_Bulk>
    </statBases>
    <costList>
      <Steel>40</Steel>
      <Component>1</Component>
      <WoodLog>2</WoodLog>
    </costList>
    <recipeMaker>
      <researchPrerequisite>Gunsmithing</researchPrerequisite>
    </recipeMaker>
    <techLevel>Industrial</techLevel>
    <generateCommonality>0.3</generateCommonality>
    <verbs>
    <li>
      <verbClass>Verb_Shoot</verbClass>
      <hasStandardCommand>true</hasStandardCommand>
      <defaultProjectile>EFR_Bullet_9x18mm</defaultProjectile>
      <warmupTime>1</warmupTime>
      <range>12</range>
      <soundCast>ShotAutopistol</soundCast>
      <muzzleFlashScale>9</muzzleFlashScale>
    </li>
    </verbs>
    <weaponTags>
 <!--<li>CE_Sidearm</li>-->
      <li>SimpleGun</li>
    </weaponTags>
 <tools>
      <li>
        <label>grip</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
      </li>
    </tools>
  </ThingDef>

